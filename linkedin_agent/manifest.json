{
  "id": "linkedin-agent",
  "name": "LinkedIn Automation Agent",
  "description": "Generate personalized LinkedIn connection notes and messages using AI. Works with OpenClaw browser control for seamless automation.",
  "version": "3.0.0",
  "author": "Sam (@sujalmanpara)",
  "category": "automation",
  "tags": ["linkedin", "outreach", "automation", "ai-personalization", "networking", "openclaw"],
  "requiredEnv": [
    "LLM_API_KEY"
  ],
  "optionalEnv": [
    "LLM_PROVIDER",
    "LLM_MODEL"
  ],
  "trigger": [
    "linkedin",
    "send connection",
    "connect on linkedin",
    "linkedin message",
    "reach out on linkedin",
    "connect with"
  ],
  "timeout": 60,
  "maxConcurrency": 5,
  "supportsHumanInLoop": false,
  "enabled": true
}
